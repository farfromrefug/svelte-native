  "license": "MIT",
  "peerDependencies": {
    "@nativescript/core": "~7.0.0",
    "svelte": "~3.24.1"
  },
  "devDependencies": {
    "glob": "^7.1.6",
    "rollup": "^2.26.10",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "svelte": "~3.24.1",
    "@nativescript/core": "~7.0.0",
    "@nativescript/types": "~7.0.1",
    "tslib":"2.1.0",
    "typescript": "^4.0.2",
    "rimraf": "^2.6.3"
  }
}
